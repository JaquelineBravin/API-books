{
  "expo": {
    "name": "API-books",
    "slug": "API-books",
    "scheme": "api-books",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bravinxha.APIbooks"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bravinxha.APIbooks"
    },
    "experiments": {
      "typedRoutes": true
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-notifications"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3c21e573-98f4-480a-bf40-0af262a3a9a9"
      }
    },
    "owner": "bravinxha"
  }
}
